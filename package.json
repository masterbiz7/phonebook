{
  "name": "react-semantic-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.0",
    "redux": "^4.0.0",
    "semantic-ui-react": "^0.79.1"
  },
  "devDependencies": {
    "better-console": "*",
    "concurrently": "^3.5.1",
    "del": "*",
    "extend": "*",
    "gulp": "*",
    "gulp-autoprefixer": "*",
    "gulp-chmod": "*",
    "gulp-clean-css": "*",
    "gulp-clone": "*",
    "gulp-concat": "*",
    "gulp-concat-css": "*",
    "gulp-copy": "*",
    "gulp-dedupe": "*",
    "gulp-flatten": "*",
    "gulp-header": "*",
    "gulp-help": "*",
    "gulp-if": "*",
    "gulp-less": "*",
    "gulp-notify": "*",
    "gulp-plumber": "*",
    "gulp-print": "*",
    "gulp-rename": "*",
    "gulp-replace": "*",
    "gulp-rtlcss": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-watch": "*",
    "map-stream": "*",
    "react-scripts": "1.0.13",
    "require-dot-file": "*",
    "run-sequence": "*",
    "yamljs": "*"
  },
  "scripts": {
    "semantic:start": "gulp --gulpfile src/semantic/gulpfile.js watch",
    "semantic:build": "gulp --gulpfile src/semantic/gulpfile.js build-css build-assets",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "start": "concurrently \"npm run semantic:start\" \"react-scripts start\"",
    "build": "npm run semantic:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
